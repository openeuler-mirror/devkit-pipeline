    <control>
              
        <selection default="detailed" label="Garbage Collector" name="gc-level">
                    
            <option label="Off" name="off">off</option>
                    
            <option label="Normal" name="detailed">normal</option>
                    
            <option label="All" name="all">all</option>
                  
        </selection>
              
        <condition false="false" name="gc-enabled-normal" true="true">
                    
            <or>
                          
                <test name="gc-level" operator="equal" value="normal"/>
                          
                <test name="gc-level" operator="equal" value="all"/>
                        
            </or>
                  
        </condition>
              
        <condition false="false" name="gc-enabled-all" true="true">
                    
            <test name="gc-level" operator="equal" value="all"/>
                  
        </condition>
              
        <selection default="medium" label="Memory Profiling" name="memory-profiling">
                    
            <option label="Off" name="off">off</option>
                    
            <option label="Object Allocation and Promotion" name="medium">medium</option>
                    
            <option label="All, including Heap Statistics (May cause long full GCs)" name="all">all</option>
                  
        </selection>
              
        <condition false="false" name="memory-profiling-enabled-medium" true="true">
                    
            <or>
                          
                <test name="memory-profiling" operator="equal" value="medium"/>
                          
                <test name="memory-profiling" operator="equal" value="all"/>
                        
            </or>
                  
        </condition>
              
        <condition false="false" name="memory-profiling-enabled-all" true="true">
                    
            <test name="memory-profiling" operator="equal" value="all"/>
                  
        </condition>
              
        <selection default="detailed" label="Compiler" name="compiler-level">
                    
            <option label="Off" name="off">off</option>
                    
            <option label="Normal" name="normal">normal</option>
                    
            <option label="Detailed" name="detailed">detailed</option>
                    
            <option label="All" name="all">all</option>
                  
        </selection>
              
        <condition false="true" name="compiler-enabled" true="false">
                    
            <test name="compiler-level" operator="equal" value="off"/>
                  
        </condition>
              
        <condition false="false" name="compiler-enabled-failure" true="true">
                    
            <or>
                          
                <test name="compiler-level" operator="equal" value="detailed"/>
                          
                <test name="compiler-level" operator="equal" value="all"/>
                        
            </or>
                  
        </condition>
              
        <condition false="100 ms" name="compiler-sweeper-threshold" true="0 ms">
                    
            <test name="compiler-level" operator="equal" value="all"/>
                  
        </condition>
              
        <condition name="compiler-compilation-threshold" true="1000 ms">
                    
            <test name="compiler-level" operator="equal" value="normal"/>
                  
        </condition>
              
        <condition name="compiler-compilation-threshold" true="100 ms">
                    
            <test name="compiler-level" operator="equal" value="detailed"/>
                  
        </condition>
              
        <condition name="compiler-compilation-threshold" true="0 ms">
                    
            <test name="compiler-level" operator="equal" value="all"/>
                  
        </condition>
              
        <condition name="compiler-phase-threshold" true="60 s">
                    
            <test name="compiler-level" operator="equal" value="normal"/>
                  
        </condition>
              
        <condition name="compiler-phase-threshold" true="10 s">
                    
            <test name="compiler-level" operator="equal" value="detailed"/>
                  
        </condition>
              
        <condition name="compiler-phase-threshold" true="0 s">
                    
            <test name="compiler-level" operator="equal" value="all"/>
                  
        </condition>
              
        <selection default="maximum" label="Method Sampling" name="method-sampling-interval">
                    
            <option label="Off" name="off">999 d</option>
                    
            <option label="Normal" name="normal">20 ms</option>
                    
            <option label="Maximum" name="maximum">10 ms</option>
                  
        </selection>
              
        <condition false="true" name="method-sampling-enabled" true="false">
                    
            <test name="method-sampling-interval" operator="equal" value="999 d"/>
                  
        </condition>
              
        <selection default="everyMinute" label="Thread Dump" name="thread-dump-interval">
                    
            <option label="Off" name="off">999 d</option>
                    
            <option label="At least Once" name="normal">everyChunk</option>
                    
            <option label="Every 60 s" name="everyMinute">60 s</option>
                    
            <option label="Every 10 s" name="everyTenSecond">10 s</option>
                    
            <option label="Every 1 s" name="everySecond">1 s</option>
                  
        </selection>
              
        <condition false="true" name="thread-dump-enabled" true="false">
                    
            <test name="thread-dump-interval" operator="equal" value="999 d"/>
                  
        </condition>
              
        <selection default="errors" label="Exceptions" name="exception-level">
                    
            <option label="Off" name="off">off</option>
                    
            <option label="Errors Only" name="errors">errors</option>
                    
            <option label="All Exceptions, including Errors" name="all">all</option>
                  
        </selection>
              
        <condition false="false" name="enable-errors" true="true">
                    
            <or>
                          
                <test name="exception-level" operator="equal" value="errors"/>
                          
                <test name="exception-level" operator="equal" value="all"/>
                        
            </or>
                  
        </condition>
              
        <condition false="false" name="enable-exceptions" true="true">
                    
            <test name="exception-level" operator="equal" value="all"/>
                  
        </condition>
              
        <selection default="medium" label="Memory Leak Detection" name="memory-leak-detection">
                    
            <option label="Off" name="off">off</option>
                    
            <option label="Object Types" name="minimal">minimal</option>
                    
            <option label="Object Types + Allocation Stack Traces" name="medium">medium</option>
                    
            <option label="Object Types + Allocation Stack Traces + Path to GC Root" name="full">full</option>
                  
        </selection>
              
        <condition false="true" name="memory-leak-detection-enabled" true="false">
                    
            <test name="memory-leak-detection" operator="equal" value="off"/>
                  
        </condition>
              
        <condition false="false" name="memory-leak-detection-stack-trace" true="true">
                    
            <or>
                          
                <test name="memory-leak-detection" operator="equal" value="medium"/>
                          
                <test name="memory-leak-detection" operator="equal" value="full"/>
                        
            </or>
                  
        </condition>
              
        <condition false="0 ns" name="memory-leak-detection-cutoff" true="1 h">
                    
            <test name="memory-leak-detection" operator="equal" value="full"/>
                  
        </condition>
              
        <text contentType="timespan" label="Synchronization Threshold" minimum="0 s" name="synchronization-threshold">10 ms</text>
              
        <text contentType="timespan" label="File I/O Threshold" minimum="0 s" name="file-io-threshold">10 ms</text>
              
        <text contentType="timespan" label="Socket I/O Threshold" minimum="0 s" name="socket-io-threshold">10 ms</text>
              
        <flag label="Class Loading" name="class-loading-enabled">false</flag>
            
    </control>
    
</configuration>
